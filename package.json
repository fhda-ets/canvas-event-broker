    "case": "^1.4.1",
    "config": "^1.21.0",
    "cron": "^1.2.1",
    "es6-error": "^3.1.0",
    "express": "^4.14.0",
    "fast-csv": "^2.3.0",
    "fhda-pubsub-logging": "git+http://stash.ad.fhda.edu/scm/npmp/fhda-pubsub-logging.git#1.0.1",
    "fs-jetpack": "^0.9.2",
    "http-link-header": "^0.8.0",
