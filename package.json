  "devDependencies": {
    "docdash": "^0.4.0",
    "eslint": "^3.5.0",
    "jsdoc": "^3.4.2",
    "jsdoc-strip-async-await": "^0.1.0",
    "mocha": "^3.0.2",
    "mocha-bamboo-reporter": "^1.1.1",
    "mocha-better-spec-reporter": "^3.0.2",
    "mocha-eslint": "^3.0.1",
    "should": "^11.1.0"
  },
  "scripts": {
    "docs": "rm -Rf docs && node_modules/.bin/jsdoc -c jsdoc.json",
    "pm2-kill": "node_modules/pm2/bin/pm2 kill",
    "pm2-start": "node_modules/pm2/bin/pm2 start --force --no-daemon --no-vizion --restart-delay 5000 src/Main.js",
    "pm2-status": "node_modules/pm2/bin/pm2 status --no-daemon",
    "pm2-stop": "node_modules/pm2/bin/pm2 stop --no-daemon Main",
    "start": "node src/Main.js",
    "test": "export NODE_APP_INSTANCE=mocha && node_modules/mocha/bin/mocha -R mocha-better-spec-reporter src/tests/Bootstrap.js src/tests/*Tests.js",
    "test-single": "export NODE_APP_INSTANCE=mocha && node_modules/mocha/bin/mocha -R mocha-better-spec-reporter src/tests/Bootstrap.js"
  }
}
